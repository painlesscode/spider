{
    "name": "painlesscode/spider",
    "type": "library",
    "description": "Resource manager for laravel",
    "autoload": {
        "psr-4": {
            "Painlesscode\\Spider\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hazzaz Bin Faiz",
            "email": "hazzazbinfaiz@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0",
        "painlesscode/module-connector": "dev-main",
        "painlesscode/reporter": "dev-main"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Painlesscode\\Spider\\SpiderServiceProvider"
            ]
        }
    },
    "repositories": {
        "reporter": {
            "type": "vcs",
            "url": "https://github.com/painlesscode/reporter.git"
        },
        "module-connector": {
            "type": "vcs",
            "url": "https://github.com/painlesscode/module-connector.git"
        }
    }
}
